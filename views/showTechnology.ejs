<% layout('layouts/boilerplate') %>
    <div class="container d-flex justify-content-center mt-50 mb-50">
        <div class="row">
            <div class="col-md-4 mt-2 d-flex align-items-stretch py-3">
                <div class="card">
                    <div class="card-body">
                        <div class="card-img-actions"> <img src="<%= products[0].image %>" class="card-img img-fluid"
                                width="90" height="300" alt=""> </div>
                    </div>
                    <div class="card-body bg-light text-center">
                        <div class="mb-2">
                            <h6 class="font-weight-semibold text-dark mb-2"> <a href="/products/<%=products[0]._id%>"
                                    class="text-default mb-2" data-abc="true">
                                    <%= products[0].title %>
                                </a> </h6>
                            <p class="text-muted">
                                <%=products[0].category%>
                            </p>
                        </div>
                        <h3 class="mb-0 font-weight-semibold">
                            $<%=products[0].price%>
                        </h3>
                        <div> <i class="fa fa-star star"></i> <i class="fa fa-star star"></i> <i
                                class="fa fa-star star"></i> <i class="fa fa-star star"></i> </div>
                        <div class="text-muted mb-3">
                            Rating: <%=products[0].rating.score%>
                        </div>
                    </div>
                </div>
            </div>
